- page_title _('SAML SSO for %{group_name}') % { group_name: @group_name }

%div
  = render 'tabs', resource: resource, current_user: current_user
  .tab-content
    = render 'register_pane', resource: resource
    - if current_user && current_user.email == resource.email
      = render 'authorize_pane'
