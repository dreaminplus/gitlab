- page_title 'Geo Projects'
- params[:sync_status] ||= []

= render 'admin/geo/shared/filter_nav', replicable_type: _('projects'), replicable_controller: 'admin/geo/projects', actions: @actions
- case params[:sync_status]
- when 'failed'
  = render(partial: 'failed')
- when 'pending'
  = render(partial: 'pending')
- when 'synced'
  = render(partial: 'synced')
- else
  = render(partial: 'all')
